{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/models/(.*)",
      "destination": "/public/models/$1"
    },
    {
      "source": "/vendor/(.*)",
      "destination": "/public/vendor/$1"
    },
    {
      "source": "/utils/(.*)",
      "destination": "/public/utils/$1"
    }
  ]
}