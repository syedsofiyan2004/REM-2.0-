{
  "builds": [
    {
      "src": "api/chat.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py", 
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/models/(.*)",
      "destination": "/public/models/$1"
    },
    {
      "source": "/vendor/(.*)",
      "destination": "/public/vendor/$1"
    },
    {
      "source": "/utils/(.*)",
      "destination": "/public/utils/$1"
    }
  ]
}